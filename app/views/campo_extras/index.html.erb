<p id="notice"><%= notice %></p>

<h1>Listing Campo Extras</h1>

<table>
  <thead>
    <tr>
      <th>Label</th>
      <th>Campo tipo</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @campo_extras.each do |campo_extra| %>
      <tr>
        <td><%= campo_extra.label %></td>
        <td><%= campo_extra.campo_tipo %></td>
        <td><%= campo_extra.user_id %></td>
        <td><%= link_to 'Show', campo_extra %></td>
        <td><%= link_to 'Edit', edit_campo_extra_path(campo_extra) %></td>
        <td><%= link_to 'Destroy', campo_extra, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Campo extra', new_campo_extra_path %>
