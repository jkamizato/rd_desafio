<h1>Campo Extra</h1>
<%= link_to 'Novo Campo extra', new_campo_extra_path, class: 'btn btn-primary pull-right' %>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Label</th>
      <th>Campo tipo</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @campo_extras.each do |campo_extra| %>
      <tr>
        <td><%= campo_extra.label %></td>
        <td><%= campo_extra.campo_tipo %></td>
        <td><%= campo_extra.user_id %></td>
        <td><%= link_to 'Show', campo_extra, class: 'btn btn-default' %></td>
        <td><%= link_to 'Edit', edit_campo_extra_path(campo_extra), class: 'btn btn-primary' %></td>
        <td><%= link_to 'Destroy', campo_extra, method: :delete, data: { confirm: 'VocÃª tem certeza?' }, class: 'btn btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


